{
  "name": "aws-sam-ts-project",
  "version": "1.0.0",
  "description": "AWS SAM project with Node.js and TypeScript for generating reports and uploading to S3",
  "main": "app.js",
  "repository": "https://github.com/awslabs/aws-sam-cli/tree/develop/samcli/local/init/templates/cookiecutter-aws-sam-hello-nodejs",
  "author": "Tuan Tran",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "start": "sam local start-api",
    "test": "npm run compile && npm run unit",
    "deploy": "sam build --debug && sam deploy --s3-bucket sample-csv-bucket --guided"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1692.0",
    "esbuild": "^0.14.14",
    "json2csv": "^5.0.7"
  },
  "devDependencies": {
    "@jest/globals": "^29.2.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.2.0",
    "@types/json2csv": "^5.0.7",
    "@types/node": "^18.11.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.2.1",
    "prettier": "^2.5.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
